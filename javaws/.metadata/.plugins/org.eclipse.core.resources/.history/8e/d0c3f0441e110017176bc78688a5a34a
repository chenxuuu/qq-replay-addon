package com.sweetcreeper.qqplugin;
import org.bukkit.plugin.java.JavaPlugin;

public class Qqplugin extends JavaPlugin
{
	@Override//重写父类的方法
	public void onEnable(){
		getLogger().info("QQGroupMessagePlugin is started successfully!"); //在控制台中输出插件已加载完毕
	}
	@Override
	public void onDisable(){
		getLogger().info("QQGroupMessagePlugin is stoped successfully!");
	}
}
